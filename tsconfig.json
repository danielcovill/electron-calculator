{
  "compilerOptions": {
    "target": "ES2016",
    "module": "commonjs",
    "sourceMap": true,
    "removeComments": false,
    "strict": true,
    "strictNullChecks": false,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true
  }
}
